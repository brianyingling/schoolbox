.row
  .12-large.columns
    %h1 Students
.row
  .12-large.columns
    %table#students
      %thead
        %tr
          %td Add Student
          %td First Name
          %td Last Name
          %td Email
          %td Gender
          %td Username
      %tbody
        - @students.each do |student|
          %tr
            %td= check_box_tag("")
            %td= student.first_name
            %td= student.last_name
            %td= student.email
            %td= student.gender
            %td= student.address